<% layout('layout/boilerplate') %>


<link rel="stylesheet" href="/css/admin.css">
    <header class="header" data-header>
        <div class="container">
            <div class="logo">
                <a href="/index.html"><img src="assets/images/firm2.svg" width="300" height="70" id="header"
                        class="img-logo" alt="FUNDING FIRM LOGO"></a>
            </div>
            <nav class="navbar" data-navbar>
                <ul class="nav-list">
                    <li class="nav-item"><img src="assets/images/185038_home_house_icon.png" class="img-link" alt=""><a
                            href="/dashboard.html" class="nav-link" data-nav-link
                            style="border-bottom: 1px solid black;">
                            DashBoard</a></li>
                    <li class="nav-item"><img src="assets/images/9499384_no_deposit_lie_bribe_icon.png" class="img-link"
                            alt=""><a href="/deposite.html" class="nav-link" data-nav-link>Deposite</a></li>
                    <li class="nav-item"><img src="assets/images/211635_card_icon.png" class="img-link" alt=""><a
                            href="/withdraw.html" class="nav-link" data-nav-link>Withdrawl</a></li>
                    <li class="nav-item"><img src="assets/images/172475_headset_support_icon.png" class="img-link"
                            alt=""><a href="/support.html" class="nav-link" data-nav-link>support</a></li>
                    <li class="nav-item nav-profile"><img
                            src="assets/images/8324223_ui_essential_app_avatar_profile_icon.png" class="img-link"
                            width="30" alt=""><a class="nav-link  " href="/">profile</a>
                    </li>
                </ul>
                </ul>
            </nav>
            <div class="cta">
                <button class="nav-toggle-btn" aria-label="Toggle menu" data-nav-toggler>
                    <span class="line line-1" ;"></span>
                    <span class="line line-2" ;"></span>
                    <span class="line line-3" ;"></span>
                </button>
                <a href="#"><img src="assets/images/8324223_ui_essential_app_avatar_profile_icon.png" class="nav-avatar"
                        width="30" alt=""></a>
            </div>
        </div>

    </header>
    <main>
        <table>
            <thead>
                <th>FullName</th>
                <th>Username</th>
                <th>Email address</th>
            </thead>
            <tbody>
                <tr>
                    <% for(let user of users){ %>
                    <td> <%= user.fname%> : <%= user.lname %></td>
                    <td><%= user.username %></td>
                    <td><%= user.email %> </td>
                    <% } %>
                </tr>
            </tbody>
        </table>
    </main>
